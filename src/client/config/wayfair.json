{
  "brand_id": "wayfair",
  "brand_name": "Wayfair",
  "logo_url": "https://upload.wikimedia.org/wikipedia/commons/0/0a/Wayfair_logo_with_tagline.png",
  "dataTransform": {
    "dataPath": "purchase_history",
    "fieldMappings": [
      {
        "outputKey": "brand",
        "sourcePath": "brand",
        "defaultValue": "Wayfair"
      },
      {
        "outputKey": "order_id",
        "sourcePath": "order_id",
        "defaultValue": "N/A"
      },
      {
        "outputKey": "order_date",
        "sourcePath": "order_date",
        "defaultValue": "N/A",
        "transform": "date"
      },
      {
        "outputKey": "order_total",
        "sourcePath": "total_price",
        "transform": "string",
        "extractPattern": "\\$([0-9]+\\.?[0-9]*)",
        "formatTemplate": "${value}",
        "defaultValue": "$0.00"
      },
      {
        "outputKey": "product_names",
        "sourcePath": "product_names",
        "defaultValue": "Unknown Product"
      },
      {
        "outputKey": "image_urls",
        "sourcePath": "image_urls",
        "defaultValue": ""
      }
    ]
  }
}
