{
  "brand_id": "amazon",
  "brand_name": "Amazon",
  "logo_url": "https://i.pinimg.com/originals/01/ca/da/01cada77a0a7d326d85b7969fe26a728.jpg",
  "is_mandatory": true,
  "schema": [
    {
      "name": "email",
      "type": "email",
      "prompt": "Email"
    },
    {
      "name": "password",
      "type": "password",
      "prompt": "Password"
    },
    {
      "name": "continue,sign_in",
      "type": "click",
      "prompt": "Sign In",
      "dependsOnFields": "email"
    }
  ],
  "dataTransform": {
    "dataPath": "bundles.1.content",
    "fieldMappings": [
      {
        "outputKey": "brand",
        "sourcePath": "brand",
        "defaultValue": "Amazon"
      },
      {
        "outputKey": "order_date",
        "sourcePath": "order_date",
        "transform": "date"
      },
      {
        "outputKey": "order_total",
        "sourcePath": "order_total",
        "defaultValue": "N/A"
      },
      {
        "outputKey": "order_id",
        "sourcePath": "order_id",
        "defaultValue": "N/A"
      },
      {
        "outputKey": "product_names",
        "sourcePath": "product_names",
        "defaultValue": "N/A"
      },
      {
        "outputKey": "image_urls",
        "sourcePath": "image_urls",
        "defaultValue": "N/A",
        "transform": "image"
      },
      {
        "outputKey": "max_return_dates",
        "sourcePath": "return_window_dates",
        "defaultValue": "N/A",
        "transform": "date"
      }
    ]
  }
}
